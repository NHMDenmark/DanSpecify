    <field type="year" size="4" value="YEAR" byyear="true"/>
    <field type="separator" size="1" value="-"/>
    <field type="alphanumeric" size="2" value="AA"/>
    <field type="separator" size="1" value="-"/>
    <field type="alphanumeric" size="3" value="AAA"/>
  </format>

