        <viewdef
            type="form"
            name="Collection Object"
            class="edu.ku.brc.specify.datamodel.CollectionObject"
            gettable="edu.ku.brc.af.ui.forms.DataGetterForObj"
            settable="edu.ku.brc.af.ui.forms.DataSetterForObj">
            <desc><![CDATA[The Collection Object form.]]></desc>
            <enableRules/>
            
            <columnDef>100px,2px,195px,5px,96px,2px,210px,5px,94px,2px,116px,15px,p:g</columnDef>
            <columnDef os="lnx">115px,2px,195px,5px,125px,2px,210px,5px,125px,2px,131px,15px,p:g</columnDef>
            <columnDef os="mac">130px,2px,215px,5px,128px,2px,260px,5px,138px,2px,145px,15px,p:g</columnDef>
            <columnDef os="exp">p,2px,min(p;250px),5px:g,p,2px,p:g,5px:g,p,2px,p:g(2),p,p:g</columnDef>
            <rowDef auto="true" cell="p" sep="2px"/>

            <rows>
                <row>
				<!--Catalog Number	-->
                    <cell type="label" labelfor="1"/>
                    <cell type="field" id="1" name="reservedText" uitype="text"/>
                    
				<!--Exhibition Object Nr -->
					<cell type="label" labelfor="reservedText3"/>
                    <cell type="field" id="reservedText3" name="reservedText3" uitype="text"/>
                </row>
                
				
				<row>
				<!--AltCatalog Number	-->
                    <cell type="label" labelfor="14"/>
                    <cell type="field" id="14" name="altCatalogNumber" uitype="text"/>
					
				<!--Registered by -->
                    <cell type="label" labelfor="4"/>
                    <cell type="field" id="4" name="cataloger" uitype="querycbx" initialize="name=Agent;title=Catalog Agent" isrequired="true"/>
                </row>
				
			
                <row>
				<!--Home Collection -->
                    <cell type="label" labelfor="2"/>
                    <cell type="field" id="2" name="reservedText2" uitype="combobox" picklist="Collections" colspan="1"/>
                </row>
				
				<row>
				<!--Dimensions -->
					<cell type="label" labelfor="height"/>
					<cell type="field" id="height" name="number1" uitype="float"/>
					<cell type="label" labelfor="width"/>
					<cell type="field" id="width" name="reservedInteger3" uitype="int"/>
					<cell type="label" labelfor="depth"/>
					<cell type="field" id="depth" name="number2" uitype="float"/>


				</row>
				
				<row>
				<!--Dimensions2 -->
					<cell type="label" labelfor="diameter"/>
					<cell type="field" id="diameter" name="integer1" uitype="int"/>
					<cell type="label" labelfor="weight"/>
					<cell type="field" id="weight" name="integer2" uitype="int"/>

				</row>
				
				<row>
				<!--Additional information-->
					<cell type="label" labelfor="coremarks"/> 
                    <cell type="field" id="coremarks" name="remarks" uitype="textareabrief" colspan="15" rows="2"/>
                </row>
				
				
				<row>
					<cell type="subview" viewname="Determination" id="6" name="determinations" colspan="13" rows="5"/>
				</row>
	
				
                <row>
                    <cell type="subview" id="attachments" viewname="ObjectAttachment" name="collectionObjectAttachments" colspan="15" initialize="icon=CollectionObjectAttachment" defaulttype="icon"/>
                </row>
				
				
                <row>
                    <cell type="subview" viewname="Preparation" id="8" name="preparations" colspan="15" rows="3"/>
                </row>


				<row>
					<cell type="subview" viewname="TreatmentEvent" id="te" name="treatmentEvents" defaulttype="table" colspan="15" rows="3" />
                </row>
                

				<row>
					<cell type="subview" viewname="CollectingEvent" id="ce" name="collectingEvent" colspan="13" rows="5"/>
				</row>			
				
				<row>
					<cell type="subview" id="19" viewname="CollectionObjectProperty" name="collectionObjectProperties" colspan="13"/>
				</row>
				
				
				<row>
                    <cell type="subview" viewname="ConservDescription" id="cd" name="conservDescriptions" defaulttype="table" colspan="15" rows="3"/>
                </row>
				
				<!--
				<row>
					<cell type="subview" id="15" viewname="ExhibitsAttributes" name="collectionObjectAttribute" colspan="13"/>
				</row>

				<row>
					<cell type="label" labelfor="ce"/>
                    <cell type="subview" viewname="ConservEvent" id="ce" name="conservEvent"  colspan="17" rows="5"/>
                </row>

				
				<row>
					<cell type="label" labelfor="oj"/>
					<cell type="field" id="oj" name="objectCondition" uitype="text"/>
					<cell type="label" labelfor="rest"/>
					<cell type="field" id="rest" name="restrictions" uitype="text"/>
				</row>
				
				<row>
					<cell type="subview" viewname="TreatmentEvent" id="te" name="treatmentEvents" initialize="btn=true"/>
				</row>
				
				
				-->
				
	
                <row>
                    <cell type="separator" label="" colspan="12"/>
                </row>
                <row>
                    <cell type="label" labelfor="9"/>
                    <cell type="field" id="9" name="createdByAgent" uitype="label" readonly="true" uifieldformatter="Agent"/>
                    <cell type="label" labelfor="12"/>
                    <cell type="field" id="12" name="timestampCreated" uitype="label" readonly="true"/>
                </row>
				<row>				
                    <cell type="label" labelfor="10"/>
                    <cell type="field" id="10" name="modifiedByAgent" uitype="label" readonly="true"  uifieldformatter="Agent"/>
                    <cell type="label" labelfor="11"/>
                    <cell type="field" id="11" name="timestampModified" uitype="label" readonly="true"/>
				</row>
